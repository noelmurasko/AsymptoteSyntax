%YAML 1.2
---
# Provides Sublime Text syntax highlighting for
# Asymptote: The Vector Graphics Language.
# For more information on Asymptote, see https://asymptote.sourceforge.io.

# Syntax highlighting supports Asymptote Version 3.05-6 and lower. Later
# versions may contain grammar not currently recognized.

name: Asymptote
file_extensions: [asy]
scope: source.asy

contexts:

  comment.line.asy:
    - meta_scope: comment.line.asy
    - match: "\n"
      pop: true

  comment.block.asy:
    - meta_scope: comment.block.asy
    - match: "\\*/"
      pop: true

  string.quoted.double.asy:
    - meta_scope: string.quoted.double.asy
    - match: '"'
      pop: true

  string.quoted.single.asy:
    - meta_scope: string.quoted.single.asy
    - match: "'"
      pop: true

  main:

    - match: "//"
      push: comment.line.asy

    - match: "/\\*"
      push: comment.block.asy

    - match: "'"
      push: string.quoted.single.asy

    - match: '"'
      push: string.quoted.double.asy

    - match: '//'
      scope: comment.line.double-slash.asy

    - match: '\b(const|static|explicit|struct|typedef)\b'
      scope: storage.modifier.asy

    - match: '\+|\||\^|-|\*|/|%|#|!'
      scope: keyword.operator.arithmetic.asy

    - match: '&|==|!=|<=|<|>=|>|::'
      scope: keyword.operator.logical.asy

    - match: '\.\.\.|\.\.|--|!|---|\^\^|::|\b(new|operator)\b'
      scope: keyword.operator.asy

    - match: '\b(if|else|while|for|do|break|return|continue|unravel)\b'
      scope: keyword.control.asy

    - match: '\b(import|include|as|access|from)\b'
      scope: keyword.other.asy

    - match: '\b(\d*)(\.?)\d+'
      scope: constant.numeric.asy

    - match: '\b(and|
                |controls|
                |tension|
                |atleast|
                |curl|
                |struct|
                |typedef|
                |using|
                |new|
                |quote|
                |static|
                |autounravel|
                |public|
                |private|
                |restricted|
                |this|
                |explicit|
                |true|
                |false|
                |null|
                |cycle|
                |newframe|
                |operator)\b'
      scope: keyword.other.asy

    - match: '\b(Braid|
                |FitResult|
                |Label|
                |Legend|
                |Solution|
                |TreeNode|
                |abscissa|
                |arc|
                |arrowhead|
                |arrowhead3|
